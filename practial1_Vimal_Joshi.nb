          RowBox[{"If", "[", 
           RowBox[{
            RowBox[{
             RowBox[{
              RowBox[{"f", "[", "m", "]"}], "*", 
              RowBox[{"f", "[", "x1", "]"}]}], ">", "0"}], ",", 
